{
  "name": "miro-dashboard",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "miro",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:cps": "webpack-dev-server --inline --progress --config build/webpack.dev-cps.conf.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "build": "node build/build.js",
    "build:dev": "node build/build.dev.js",
    "build:mylaps:dev": "node build/build.dev.js",
    "build:mylaps": "node build/build-mylaps.js",
    "build:cps:dev": "node build/build-cps.dev.js",
    "build:cps:prod": "node build/build-cps.js",
    "build:cps": "node build/build-cps.js",
    "deploy:cps:dev": "node build/build-cps.dev.js && aws s3 sync ./dist s3://miro-cps-dashboard-dev --profile new --delete --acl public-read",
    "deploy:cps:prod": "node build/build-cps.js && aws s3 sync ./dist s3://miro-cps-dashboard-prod --profile new --delete --acl public-read && aws cloudfront create-invalidation --distribution-id E1HHIINB6U6BJC --paths \"/*\" --profile runnertag"
  },
